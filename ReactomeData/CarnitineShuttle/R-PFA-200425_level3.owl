    <bp:year rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1997</bp:year>
    <bp:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Human acetyl-CoA carboxylase 2. Molecular cloning, characterization, chromosomal mapping, and evidence for two isoforms.</bp:title>
